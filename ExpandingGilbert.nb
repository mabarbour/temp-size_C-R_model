  3.6713929216805267`*^9, 3.671392930634766*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"dRdt", "[", 
    RowBox[{"R_", ",", "C_", ",", "T_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"r", "[", "T", "]"}], " ", "R", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       FractionBox["R", 
        RowBox[{"K", "[", "T", "]"}]]}], ")"}]}], "-", 
    RowBox[{
     RowBox[{"f", "[", 
      RowBox[{"R", ",", "T"}], "]"}], " ", "R", " ", "C"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"dCdt", "[", 
     RowBox[{"R_", ",", "C_", ",", "T_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"e", " ", 
      RowBox[{"f", "[", 
       RowBox[{"R", ",", "T"}], "]"}], "R", " ", "C"}], "-", 
     RowBox[{
      RowBox[{"m", "[", 
       RowBox[{"C", ",", "T"}], "]"}], "C"}]}]}], ";"}], " "}]}], "Input",
 CellChangeTimes->{{3.6713850329354963`*^9, 3.671385098476318*^9}, {
  3.671385132683538*^9, 3.671385197169901*^9}}],

Cell["\<\
where R is biomass of resource, C is biomass of consumer, T is temperature, K \
is resource carrying capacity, f is the functional response, e is the \
conversion efficiency of resources into new consumers, and m is consumer \
mortality.  \
\>", "Text",
 CellChangeTimes->{{3.671392880032325*^9, 3.671393002826417*^9}}],

Cell["\<\
BCR at a given T, as defined by Gilbert (Eqn 5),\
\>", "Text",
 CellChangeTimes->{{3.671393083172274*^9, 3.671393096918651*^9}, {
   3.671393170303336*^9, 3.6713931999730453`*^9}, 3.671393383820503*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"BCR", "[", "T_", "]"}], ":=", 
  FractionBox[
   RowBox[{"e", " ", 
    RowBox[{"a", "[", "T", "]"}], 
    RowBox[{"K", "[", "T", "]"}]}], 
   RowBox[{"m", "[", "T", "]"}]], " "}]], "Input",
 CellChangeTimes->{{3.671393098677845*^9, 3.671393157181126*^9}}],

Cell["\<\
