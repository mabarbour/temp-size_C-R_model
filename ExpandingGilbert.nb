     RowBox[{
      RowBox[{"m", "[", 
       RowBox[{"C", ",", "T"}], "]"}], "\[Rule]", 
      RowBox[{"m", "[", "T", "]"}]}]}], ",", 
    RowBox[{"{", 
     RowBox[{"R", ",", "C"}], "}"}]}], "]"}]}]], "Input",
